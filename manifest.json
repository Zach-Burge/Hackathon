//this is a test for my manifest file
{
  "manifest_version": 2,
  "name": "IMLeagues AD Blocker",
  "version": "1.0",
  "description": "Blocks adds on the website IM-Leagues",
  "author": "Zach Burge, Cole Elfstrom, Graham Sabin, Ian Flury",
  "browser_action": {
    "default_title": "ImLeagues AdBlocker"
  },
  "icons": {
    "16": "adBlock16.png",
    "48": "adBlock48.png",
    "128": "adBlock128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "<all_urls>",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [ "background.js" ]
  }
}